{
  "name": "ownkeeper-backend",
  "version": "1.0.0",
  "description": "Ownkeeper backend",
  "main": "server.js",
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && node build/server.js",
    "start": "nodemon ./src/server.ts",
    "set:dev_env": "SET NODE_ENV=dev & dotenv -e ./config/config.dev.env",
    "set:prod_env": "SET NODE_ENV=prod & dotenv -e ./config/config.prod.env",
    "start:dev": "SET NODE_ENV=dev & dotenv -e ./config/config.dev.env & nodemon --watch src/**/* -e ts,json --exec node --inspect -r ts-node/register ./src/server.ts ",
    "start:prod": "SET NODE_ENV=prod & dotenv -e ./config/config.prod.env & npm run build",
    "prisma:migrate": "prisma migrate dev --skip-seed",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "prisma:seed": "prisma db seed --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:reset": "prisma migrate reset"
  },
  "keywords": [
    "ownkeeper",
    "backend"
  ],
  "author": "Prodip",
  "license": "ISC",
  "dependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/hpp": "^0.2.2",
    "@types/morgan": "^1.9.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.0.0",
    "express": "^4.17.3",
    "express-rate-limit": "^6.3.0",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "hpp": "^0.2.3",
    "morgan": "^1.10.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@prisma/client": "^3.11.0",
    "@types/node": "^17.0.21",
    "nodemon": "^2.0.15",
    "prisma": "^3.11.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
